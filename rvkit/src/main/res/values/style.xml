<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MultiStatusLayout">
        <attr format="reference" name="loadingView"/>
        <attr format="reference" name="emptyView"/>
        <attr format="reference" name="errorView"/>
        <attr format="enum" name="viewState">
            <enum name="more" value="0"/>
            <enum name="error" value="1"/>
            <enum name="empty" value="2"/>
            <enum name="loading" value="3"/>
        </attr>
    </declare-styleable>
</resources>